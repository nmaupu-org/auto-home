background yes
out_to_console yes
out_to_x no
update_interval 1
update_interval_on_battery 2
use_spacer left
 
TEXT
^fg(\#9dc3f9)^i(/home/nmaupu/.xmonad/icons/net_wired.xbm)^fg(\#00aa4a) ^i(/home/nmaupu/.xmonad/icons/net_down_03.xbm)${downspeed {{ ansible_default_ipv4.interface }}}^fg(\#ff6633) ^i(/home/nmaupu/.xmonad/icons/net_up_03.xbm)${upspeed {{ ansible_default_ipv4.interface }}} \
^fg(\#9dc3f9)^i(/home/nmaupu/.xmonad/icons/bat_full_02.xbm) \
${if_match ${exec acpi -a | grep 'on-line' | wc -l} > 0}\
^fg(\#00aa4a)\
${else}\
^fg(\#ff6600)\
${endif}\
${execi 5 acpi -b | cut -d' ' -f4 | tr -d ','} \
^fg(\#9dc3f9)^i(/home/nmaupu/.xmonad/icons/cpu.xbm) ^fg(\#e8e8e8)${cpu}% \
^fg(\#9dc3f9)^i(/home/nmaupu/.xmonad/icons/mem.xbm) ^fg(\#e8e8e8)${memperc}% \
^fg(\#9dc3f9)^i(/home/nmaupu/.xmonad/icons/clock.xbm) ^fg(\#e8e8e8)${time %Y.%m.%d} ^fg(\#FF6633)${time %H:%M:%S}^fg(\#e8e8e8)/^fg(\#00ffff)${utime %R} UTC
